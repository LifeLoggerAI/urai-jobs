{
  "name": "urai-jobs",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently "npm run emulators" "npm run -w web dev"",
    "build": "npm run -w web build && npm run -w functions build",
    "emulators": "firebase emulators:start --import=./firebase-data --export-on-exit",
    "seed": "cd scripts && ts-node seed.ts",
    "deploy": "firebase deploy",
    "test": "cd packages/functions && npm test"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "firebase-tools": "^13.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
