{
  "name": "urai-jobs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "pnpm -r build",
    "dev": "pnpm --filter web dev & firebase emulators:start",
    "test": "pnpm -r test",
    "test:ci": "pnpm -r test:ci",
    "e2e": "playwright test",
    "smoke": "./scripts/smoke_urai_jobs.sh",
    "deploy:jobs": "./scripts/deploy_jobs.sh",
    "postinstall": "pnpm -r postinstall",
    "preseed": "tsc scripts/seed.ts --outDir .",
    "seed": "firebase emulators:exec \"node scripts/seed.js\""
  },
  "devDependencies": {
    "@playwright/test": "^1.58.1",
    "firebase-tools": "^13.0.0",
    "pnpm": "^8.0.0",
    "typescript": "5.5.4"
  },
  "overrides": {
    "typescript": "5.5.4"
  },
  "packageManager": "pnpm@8.15.9"
}
