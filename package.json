{
  "name": "urai-jobs-monorepo",
  "private": true,
  "scripts": {
    "install": "pnpm -r install",
    "build": "pnpm -r build",
    "build:web": "pnpm --filter web build",
    "build:functions": "pnpm --filter functions build",
    "dev": "pnpm -r --parallel dev",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "emulators": "firebase emulators:start --import=./emulators-data --export-on-exit",
    "deploy": "firebase deploy",
    "deploy:hosting": "firebase deploy --only hosting",
    "deploy:functions": "firebase deploy --only functions"
  },
  "devDependencies": {
    "firebase-tools": "^13.10.2",
    "typescript": "^5.4.5",
    "concurrently": "^8.2.2"
  }
}
