{
  "name": "urai-jobs",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "functions",
    "web"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=web\" \"npm run emulators\"",
    "build": "npm run build --workspace=web && npm run build --workspace=functions",
    "emulators": "firebase emulators:start",
    "seed": "npm run seed --workspace=functions",
    "test": "npm test --workspace=functions"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "firebase-tools": "^13.0.2",
    "ts-node": "^10.9.2"
  }
}
