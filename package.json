{
  "name": "urai-jobs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "postinstall": "cd functions && npm install && cd ../web && npm install",
    "dev": "concurrently \"npm:emulators\" \"npm:dev:web\"",
    "dev:web": "cd web && npm run dev",
    "build": "cd functions && npm run build && cd ../web && npm run build",
    "emulators": "firebase emulators:start --import=./firebase-data --export-on-exit",
    "seed": "node ./lib/seed/seed.js",
    "test:rules": "firebase emulators:exec --only firestore \"mocha --require ts-node/register --timeout 5000 functions/src/rules.test.ts\""
  },
  "dependencies": {
    "@google-cloud/vertexai": "^0.1.3",
    "firebase": "^10.7.1",
    "firebase-admin": "^11.11.1",
    "firebase-functions": "^4.9.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "firebase-functions-test": "^3.1.0",
    "firebase-tools": "^13.0.2",
    "mocha": "^10.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
