rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /resumes/{applicantId}/{applicationId}/{filename} {
      allow read: if request.auth != null && exists(/databases/$(database)/documents/admins/$(request.auth.uid));
      allow write: if request.auth != null && request.auth.uid == applicantId;
    }
  }
}
