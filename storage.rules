rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /job-payloads/{file=**} {
      allow read, write: if request.auth.token.admin == true;
    }
    match /job-logs/{file=**} {
      allow read: if request.auth.token.admin == true;
      allow write: if request.time < timestamp.date(9999, 1, 1);
    }
  }
}