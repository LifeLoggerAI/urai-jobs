rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    match /resumes/{applicantId}/{applicationId}/{filename} {
      allow read: if request.auth != null && get(/databases/(default)/documents/admins/$(request.auth.uid)).data.role == 'admin';
      allow write: if request.auth != null;
    }
  }
}
